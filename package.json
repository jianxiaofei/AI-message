{
  "name": "ai-message",
  "displayName": "AI-message",
  "description": "Generate intelligent commit messages using multiple AI providers (GitHub Copilot, Ollama, Qianwen, Wenxin, Zhipu) for Git and SVN repositories",
  "version": "0.0.43",
  "publisher": "jianxiaofei",
  "repository": {
    "type": "git",
    "url": "https://github.com/jianxiaofei/AI-message"
  },
  "author": "jianxiaofei",
  "license": "MIT",
  "homepage": "https://github.com/jianxiaofei/AI-message#readme",
  "bugs": {
    "url": "https://github.com/jianxiaofei/AI-message/issues"
  },
  "engines": {
    "vscode": "^1.104.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "keywords": [
    "svn",
    "git",
    "commit",
    "ai",
    "copilot",
    "ollama",
    "qianwen",
    "wenxin",
    "zhipu",
    "message",
    "version control",
    "scm",
    "artificial intelligence",
    "conventional commits",
    "auto-generate",
    "local llm",
    "chinese ai",
    "multi-model"
  ],
  "extensionDependencies": [],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-message.generateCommitMessage",
        "title": "生成提交信息",
        "category": "AI Message",
        "icon": {
          "dark": "./images/ai-icon-dark.svg",
          "light": "./images/ai-icon-light.svg"
        }
      },
      {
        "command": "ai-message.quickCommit",
        "title": "快速生成提交信息",
        "category": "AI Message",
        "icon": "$(zap)"
      },
      {
        "command": "ai-message.configureAI",
        "title": "配置 AI 设置",
        "category": "AI Message",
        "icon": "$(settings-gear)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ai-message.generateCommitMessage",
          "group": "z_ai_message"
        }
      ],
      "editor/context": [
        {
          "command": "ai-message.generateCommitMessage",
          "group": "z_ai_message"
        }
      ],
      "scm/title": [
        {
          "command": "ai-message.generateCommitMessage",
          "when": "scmProvider",
          "group": "navigation@1"
        }
      ],
      "scm/resourceState/context": [
        {
          "command": "ai-message.generateCommitMessage",
          "group": "1_modification",
          "when": "scmProvider"
        }
      ]
    },
    "keybindings": [
      {
        "command": "ai-message.generateCommitMessage",
        "key": "ctrl+alt+g",
        "mac": "cmd+alt+g"
      },
      {
        "command": "ai-message.quickCommit",
        "key": "ctrl+alt+q",
        "mac": "cmd+alt+q"
      }
    ],
    "configuration": {
      "title": "AI Message",
      "properties": {
        "aiMessage.ai.provider": {
          "type": "string",
          "enum": [
            "copilot",
            "ollama",
            "qianwen",
            "wenxin",
            "zhipu",
            "custom"
          ],
          "enumDescriptions": [
            "使用 VS Code GitHub Copilot（推荐）",
            "使用本地 Ollama 模型",
            "使用阿里云通义千问",
            "使用百度文心一言",
            "使用智谱AI",
            "自定义 API 接口"
          ],
          "default": "copilot",
          "description": "选择AI提供商",
          "order": 1
        },
        "aiMessage.ai.ollamaEndpoint": {
          "type": "string",
          "default": "http://localhost:11434",
          "description": "Ollama 服务器地址",
          "order": 2
        },
        "aiMessage.ai.ollamaModel": {
          "type": "string",
          "default": "qwen2.5:7b",
          "description": "Ollama 模型名称（例如：qwen2.5:7b, llama3:8b, codegemma:7b）",
          "order": 3
        },
        "aiMessage.ai.qianwenApiKey": {
          "type": "string",
          "default": "",
          "description": "通义千问 API Key",
          "order": 4
        },
        "aiMessage.ai.qianwenModel": {
          "type": "string",
          "enum": [
            "qwen-max",
            "qwen-plus",
            "qwen-turbo",
            "qwen-7b-chat",
            "qwen-14b-chat",
            "qwen-72b-chat"
          ],
          "default": "qwen-plus",
          "description": "通义千问模型",
          "order": 5
        },
        "aiMessage.ai.wenxinApiKey": {
          "type": "string",
          "default": "",
          "description": "文心一言 API Key",
          "order": 6
        },
        "aiMessage.ai.wenxinSecretKey": {
          "type": "string",
          "default": "",
          "description": "文心一言 Secret Key",
          "order": 7
        },
        "aiMessage.ai.wenxinModel": {
          "type": "string",
          "enum": [
            "ernie-4.0-8k",
            "ernie-3.5-8k",
            "ernie-speed-8k",
            "ernie-lite-8k",
            "ernie-tiny-8k"
          ],
          "default": "ernie-3.5-8k",
          "description": "文心一言模型",
          "order": 8
        },
        "aiMessage.ai.zhipuApiKey": {
          "type": "string",
          "default": "",
          "description": "智谱AI API Key",
          "order": 9
        },
        "aiMessage.ai.zhipuModel": {
          "type": "string",
          "enum": [
            "glm-4",
            "glm-4-flash",
            "glm-3-turbo",
            "codegeex-4"
          ],
          "default": "glm-4",
          "description": "智谱AI模型",
          "order": 10
        },
        "aiMessage.ai.customEndpoint": {
          "type": "string",
          "default": "",
          "description": "自定义 API 接口地址",
          "order": 11
        },
        "aiMessage.ai.customApiKey": {
          "type": "string",
          "default": "",
          "description": "自定义 API Key",
          "order": 12
        },
        "aiMessage.ai.customModel": {
          "type": "string",
          "default": "",
          "description": "自定义模型名称",
          "order": 13
        },
        "aiMessage.ai.timeout": {
          "type": "number",
          "default": 30000,
          "minimum": 5000,
          "maximum": 120000,
          "description": "API 请求超时时间（毫秒）",
          "order": 14
        },
        "aiMessage.svn.path": {
          "type": "string",
          "default": "",
          "description": "可选：自定义 svn 可执行文件路径（例如 /opt/homebrew/bin/svn）",
          "order": 15
        },
        "aiMessage.git.path": {
          "type": "string",
          "default": "",
          "description": "可选：自定义 git 可执行文件路径（例如 /usr/local/bin/git）",
          "order": 16
        },
        "aiMessage.commitTemplate": {
          "type": "string",
          "default": "",
          "description": "提交信息模板 (可选，用 {message} 占位符代表AI生成的信息)",
          "order": 17
        },
        "aiMessage.enableFallback": {
          "type": "boolean",
          "default": true,
          "description": "当AI服务不可用时，启用基于规则的后备提交信息生成",
          "order": 18
        },
        "aiMessage.commit.enableEmoji": {
          "type": "boolean",
          "default": true,
          "description": "在提交信息中启用 emoji 图标",
          "order": 19
        },
        "aiMessage.commit.enableBody": {
          "type": "boolean",
          "default": true,
          "description": "在提交信息中包含详细的正文内容",
          "order": 20
        },
        "aiMessage.commit.enableScope": {
          "type": "boolean",
          "default": true,
          "description": "在提交信息中自动推断和添加作用域 (scope)",
          "order": 21
        },
        "aiMessage.commit.language": {
          "type": "string",
          "enum": [
            "简体中文",
            "繁體中文",
            "English",
            "日本語",
            "한국어"
          ],
          "default": "简体中文",
          "description": "提交信息的语言",
          "order": 22
        },
        "aiMessage.streaming.forceOutputChannel": {
          "type": "boolean",
          "default": false,
          "description": "强制使用输出面板显示流式生成内容（调试或SVN输入框无法写入时可开启）",
          "order": 23
        },
        "aiMessage.debug.enableStreamingLog": {
          "type": "boolean",
          "default": false,
          "description": "启用详细的流式调试日志（包含fragment次数、长度、fallback情况）",
          "order": 24
        },
        "aiMessage.commit.intelligentBody": {
          "type": "boolean",
          "default": true,
          "description": "启用智能body生成（基于代码diff分析函数变更、导入变更等）",
          "order": 25
        },
        "aiMessage.commit.bodyQualityCheck": {
          "type": "boolean",
          "default": true,
          "description": "启用body质量检查（长度检查、重复内容检查、格式标准化）",
          "order": 26
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "version-patch": "npm version patch --no-git-tag-version",
    "build-and-package": "npm run version-patch && npm run package && vsce package --out releases/",
    "package-only": "vsce package --out releases/",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "esbuild": "^0.25.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
